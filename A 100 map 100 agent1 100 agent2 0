#ブロック作成を行う関数
def make_brock():
    for i in range(50):
        for j in range(50):
            
            if (i == 0 and j == 49):  # 赤色　ゴール地点とスタート地点
                fill(250, 20, 0)
               
                rect(j * 10, i * 10, 10, 10)

            elif (i == 8 and j == 7 or (i == 8 and j == 31)):  # 中間地点
                # println(j)
                fill(055, 255, 0)
                rect(j * 10, i * 10, 10, 10)

            elif (i == 49 and j == 0):  # スタートとゴール地点
                fill(0, 0, 255)
                rect(j * 10, i * 10, 10, 10)

            elif brock[i][j] == 0:
                fill(255, 255, 255)
                rect(j * 10, i * 10, 10, 10)

def path_search1():
    #path_search2()
    #エージェント１の経路探索
    global play_count_1  #関数の実行回数
    
    if play_count_1 ==0:
        start = (49, 0)  # スタート地点
        end = (8, 7)  # タスク地点 (goal)
        path = Node.astar(brock, start, end)  # A_starで探索された経路
        
        # start_2 = (0,49)  # スタート地点
        # end_2 = (8, 30)  # タスク地点 (goal)
        # path_2 = Node.astar(brock, start_2, end_2)  # A_starで探索された経路
        # print(path_2)
        play_count_1 +=1
        
    elif play_count_1 ==1:
        start = (8, 7)  # スタート地点
        end = (0, 49)  # タスク地点 (goal)
        path = Node.astar(brock, start, end)  # A_starで探索された経路

    #探索された経路のx,yを各配列に格納
    for i in range(len(path)):
        x_1.append(path[i][1])
        y_1.append(path[i][0])
            
def path_search2():
    
    #エージェント2の経路探索
    global play_count_2  #関数の実行回数
    #println(play_count_2)
    
    if play_count_2 ==0:
        start = (0,49)  #  スタート地点
        end = (8, 30)  # タスク地点 (goal)
        path = Node.astar(brock, start, end)  # A_starで探索された経路
        println(path)
        play_count_2 +=1
        

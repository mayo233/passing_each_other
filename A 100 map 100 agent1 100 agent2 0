#探索された経路を可視化する関数
def path_visual():
    global count
    for i in range(len(x)):
            fill(0,0,200)
            rect(x[i]*10, y[i]*10, 10, 10)  #探索された経路の升目に色を塗る

def agent_move1():
    global count,goal
    #print(goal)
    if count <=len(x_1) and goal ==False:
        fill(155,100,100)
        ellipse(x_1[count]*10+2.5, y_1[count]*10+4, 7, 7)  #エージェント１ 
        
        if count >=107:
            goal =True
        
    if goal ==True:
        fill(155,100,100)
        ellipse(x_1[-1]*10+2.5, y_1[-1]*10+4, 7, 7)  #エージェント１ 
        
    count+=1

def agent_move2():
    global count,goal
    print(goal)
    if count <=len(x) and goal ==False:
        fill(155,100,100)
        ellipse(x[count]*10+2.5, y[count]*10+4, 7, 7)  #エージェント2
        
    if count==108 or goal ==True:
        goal =True
        fill(155,100,100)
        ellipse(x[-1]*10+2.5, y[-1]*10+4, 7, 7)  #エージェント2
        
    count+=1

def setup():
    size(500, 500)
    frameRate(10)

def draw():
    background(0)
    make_brock()
    #path_search1()
    #path_search2()
    #path_visual()
    agent_move1()
    #agent_move2()
    
if __name__ == '__main__':
    #path_search1()

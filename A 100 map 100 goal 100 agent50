global agent1, agent2
global count,play_count
global agent
global goal


x = []
y = []
agent =[]

goal =False
agent1 = 490
count=0
play_count=0


#ブロック作成を行う関数
def make_brock():
    for i in range(50):
        for j in range(50):
            
            if (i == 0 and j == 49):  # 赤色　ゴール地点とスタート地点
                fill(250, 20, 0)
               
                rect(j * 10, i * 10, 10, 10)

            elif (i == 8 and j == 7 or (i == 8 and j == 31)):  # 中間地点
                # println(j)
                fill(055, 255, 0)
                rect(j * 10, i * 10, 10, 10)

            elif (i == 49 and j == 0):  # スタートとゴール地点
                fill(0, 0, 255)
                rect(j * 10, i * 10, 10, 10)

            elif brock[i][j] == 0:
                fill(255, 255, 255)
                rect(j * 10, i * 10, 10, 10)

def path_search():
    global play_count
    
    if play_count ==0:
        start = (49, 0)  # 49,0 スタート地点
        end = (8, 7)  # 8,7 タスク地点 (goal)
        path = Node.astar(brock, start, end)  # A_starで探索された経路
        play_count +=1
        
    elif play_count ==1:
        start = (8, 7)  #  スタート地点
        end = (0, 49)  # タスク地点 (goal)
        path = Node.astar(brock, start, end)  # A_starで探索された経路

    #探索された経路のx,yを各配列に格納
    for i in range(len(path)):
        x.append(path[i][1])
        y.append(path[i][0])
        
    
#探索された経路を可視化する関数
def path_visual():
    global count
    for i in range(len(x)):
            fill(0,0,200)
            rect(x[i]*10, y[i]*10, 10, 10)  #探索された経路の升目に色を塗る

def agent_move1():
    global count,goal
    print(goal)
    if count <=len(x) and goal ==False:
        fill(155,100,100)
        ellipse(x[count]*10+2.5, y[count]*10+4, 7, 7)  #エージェント１ 

        
    if count==108 or goal ==True:
        goal =True
        fill(155,100,100)
        ellipse(x[-1]*10+2.5, y[-1]*10+4, 7, 7)  #エージェント１ 
        
    count+=1

def agent_move2():
    global count,goal
    print(goal)
    if count <=len(x) and goal ==False:
        fill(155,100,100)
        ellipse(x[count]*10+2.5, y[count]*10+4, 7, 7)  #エージェント2
        
    if count==108 or goal ==True:
        goal =True
        fill(155,100,100)
        ellipse(x[-1]*10+2.5, y[-1]*10+4, 7, 7)  #エージェント2
        
    count+=1

def setup():
    size(500, 500)
    frameRate(20)

def draw():
    background(0)
    make_brock()
    path_search()
    #path_visual()
    agent_move1()
    agent_move2()
    

//ブロックを作る
void make_brock() {
  for (int i=0; i<50; i++) {
    for (int j=0; j<50; j++) {

      //赤色　ゴール地点とスタート地点
      if (brock[i][j] ==2) {
        fill(250, 20, 0);
        rect(j*10, i*10, 10, 10);
      }

      //緑色　タスク 中間地点
      else if (brock[i][j] ==3) {
        println(i);
        println(j);
        
        fill(155, 255, 0);
        rect(j*10, i*10, 10, 10);
      }

      //青色　
      else if (brock[i][j] ==4) {
        fill(0, 0, 255);
        rect(j*10, i*10, 10, 10);
      } 
      
      else {
        fill(255*brock[i][j]);  //0なら黒 1なら白
        rect(j*10, i*10, 10, 10);
      }
    }
  }
}

void setup() {
  size(500, 500);
  agent[0] =400;
  agent[1] =300;

  //frameRate(1);
}

void draw() {
  background(0);
  make_brock();
  //A*(); 
  //ellipse(490,70,8,8);

  agent[0] +=1;
  //move_agent();
}


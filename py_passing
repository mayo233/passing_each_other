def make_brock():
    for i in range(50):
        for j in range(50):
            
            if brock[i][j] ==2:   #赤色　ゴール地点とスタート地点
                fill(250,20,0)
                rect(j*10,i*10,10,10)
                
            elif brock[i][j] ==3:
                fill(155,255,0)
                rect(j*10,i*10,10,10)
                
            elif brock[i][j] ==4:
                fill(0,0,255)
                rect(j*10,i*10,10,10)
                
            else:
                fill(255*brock[i][j])
                rect(j*10,i*10,10,10)
    

def setup():
    size(500,500)
    
    
def draw():
    background(255)
    make_brock()

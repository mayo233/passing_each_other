void setup(){
  size(500,500);
  frameRate(60);
}

void draw(){
  background(0);
  make_brock();
  move_agent();
}

void move_agent(){
  for(int i=-1; i<2; i++){
    for(int j=-1; j<2; j++){
      if(brock[agent[0][0]+j][agent[0][1]+i]*advance_check[1+j][1+i] == 0){
        if(i>0){
          agent[0][1]+=1;
        }
        //if(i<0){
        //  agent[0][1]-=1;
        //}
        if(j>0){
          agent[0][0]+=1;
        }
        if(j<0){
          agent[0][0]-=1;
        }
      }
    }
  }
  ellipse(agent[0][0]*10+5,agent[0][1]*10+5,10,10);
}

void make_brock(){
    for(int i=0; i<50; i++){
      for(int j=0; j<50; j++){
        fill(255*brock[j][i]);
        rect(i*10,j*10,10,10);      
      }
    }
}

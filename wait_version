def path_search11():
    # path_search2()
    # エージェント１の経路探索
    # global play_count_1  # 関数の実行回数

    # if play_count_1 == 0:
        start = (49, 0)  # スタート地点  49,0
        end = (8, 7)  # タスク地点 (goal)  8,7
        path_11 = Node.astar(brock, start, end)  # A_starで探索された経路
        # print(path_11)

        # start_2 = (0,49)  # スタート地点
        # end_2 = (8, 30)  # タスク地点 (goal)
        # path_2 = Node.astar(brock, start_2, end_2)  # A_starで探索された経路
        # print(path)
        # play_count_1 += 1

    # elif play_count_1 ==1:
    # print(path)

        # 探索された経路のx,yを各配列に格納
        for i in range(len(path_11)):
            x_1.append(path_11[i][1])
            y_1.append(path_11[i][0])
            
        
def path_search12():
        start = (8, 7)  # スタート地点  8,7
        end = (0, 49)  # タスク地点 (goal) 49,0
        path_12 = Node.astar(brock, start, end)  # A_starで探索された経路
        # print(path_12)
        
        # 探索された経路のx,yを各配列に格納
        for i in range(len(path_12)):
            x_1.append(path_12[i][1])
            y_1.append(path_12[i][0])
            x_12.append(path_12[i][1])
            y_12.append(path_12[i][0])

        # print(x_12)
        
def path_search21(pos):
    
    # エージェント2の経路探索
    # global play_count_2, path_2, path_1  # 関数の実行回数
    # println(play_count_2)
    
       start = (pos, 49)  # スタート地点 0,49
       end = (8, 31)  # タスク地点 (goal)  8,31
       path_21 = Node.astar(brock, start, end)  # A_starで探索された経路
       #print(path_21)
       
        # # 探索された経路のx,yを各配列に格納
       for i in range(len(path_21)):
            x_2.append(path_21[i][1])
            y_2.append(path_21[i][0])
            
       print(y_2)
                    
        
def path_search22():
    
     global play_count_2
     
     start = (8, 31)  #  スタート地点
     end = (49,0)  # タスク地点 (goal)
     path_22 = Node.astar(brock, start, end)  # A_starで探索された経路
     
     if play_count_2 ==0:
         
        # 探索された経路のx,yを各配列に格納
        for i in range(len(path_22)):
            x_22.append(path_22[i][1])
            y_22.append(path_22[i][0])
        play_count_2+=1
        
     elif play_count_2 ==1:
        # 探索された経路のx,yを各配列に格納
        for i in range(len(path_22)):
            x_2.append(path_22[i][1])
            y_2.append(path_22[i][0])

     #print(x_2)
     

# エージェント2を入り口付近まで移動させて待機
def path_avoid():
    for i in range(len(x_12)):
        # print(x_22)
        # print(x_12)
        if x_12[i] ==x_22[i] and y_12[i] ==y_22[i]:
            return True
        
    return False
        
def path_repair(flag):
    if flag ==True:
        print("iii")
        start = (0, 49)  # スタート地点 0,49
        end = (y_22[0]+1, 49)  # タスク地点 (goal)  8,31
        path_repair = Node.astar(brock, start, end)  # A_starで探索された経路
        
        for i in range(len(path_repair)):
            x_2.append(path_repair[i][1])
            y_2.append(path_repair[i][0])
            
        # print(y_2)
        return y_22[0]+1
        # return path_repair

        
        
def agent_move1():
    global count, goal

    if count <= len(x_1) - 1  and variable.stop==False  :
        fill(155, 100, 100)
        ellipse(x_1[count] * 10 + 2.5, y_1[count] * 10 + 4, 7, 7)  # エージェント１
        
        if count !=len(x_1)-1:
            count+=1
        

def agent_move2():
    global count_2, goal_2, stop_time,stop,back,no_stop,back_count,pre_x,pre_y,pre_stop,block_num_pre,undo
    print(x_2)
        
    if (variable.count_2 <=len(x_2)-1 and (variable.collision==False)):
        fill(155, 100, 100)
        ellipse(x_2[variable.count_2] * 10 + 2.5, y_2[variable.count_2] * 10 + 4, 7, 7)  # エージェント１
        
        if variable.count_2 !=len(x_2)-1:
            variable.count_2+=1
        
    #     variable.count_2+=1
    
    
def setup():
    size(500, 500)
    frameRate(10)

def draw():
    background(0)
    textSize(40)
    # text("2", agent1[0], agent1[1]+20, 60, 600)

    MAP.make_brock()
    # path_search1()
    # path_search2()
    # path_visual()
    agent_move1()
    # avoid()
    agent_move2()
    

if __name__ == '__main__':
    path_search11()
    path_search12()
    
    # path_search21()
    path_search22()
    avoid_flag=path_avoid()
    wait_pos =path_repair(avoid_flag)
    path_search21(wait_pos)
    path_search22()
